# Debug mode - Set to True for local development, False for production
# Affects how the application behaves (e.g., local vs. remote Qdrant instance)
DEBUG=True

# Qdrant Vector Database Configuration
# Local development URL (when DEBUG=True)
QDRANT_URL=http://localhost:6333

# Qdrant API Key
# Only required when DEBUG=False (production mode)
# Leave empty for local development
QDRANT_API_KEY=your_api_key_here